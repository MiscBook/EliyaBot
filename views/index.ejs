  <!doctype html>
  <html>
  <head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width , initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />  
  <title>World Flipper Collection Tracker</title>
  <meta property="og:title" content="World Flipper Collection Tracker" />
  <meta property="og:type" content="website">
  <meta property="og:url" content="http://eliya-bot.herokuapp.com/<% if (data.listid) { %><%= data.listid %><% } %>">
  <meta property="og:image" content="http://eliya-bot.herokuapp.com/img/eliya.png" />
  <link rel="stylesheet" href="/css/normalize.css" type="text/css">
  <link rel="stylesheet" href="/css/reset.css" type="text/css">	
  <link rel="stylesheet" href="/css/common.css" type="text/css">	
  <link rel="stylesheet" href="/css/client.css" type="text/css">	
  <link rel="stylesheet" href="/css/iconfont/material-icons.css">	
  <style>

          </style>
      </head>

  <body class="body">
      <div class="wrapper">
      <div class="toolbar">
      <ul class="btnList">
          <li id="btnSave" class="button">Save</li>
          <li id="btnGetShareURL" class="button">Generate Share URL</li>		
          </ul>

      </div>		
          <h1 class="title">World Flipper Collection Tracker</h1>		
          <article id="chars">
              <section id="charRarity5">
				<h2>★★★★★</h2>
				<span class="score"></span>
				<div class="btnSelectAll button">Select All</div>
				<ul class="rarity5 charList unitList"></ul>
              </section>
              <section id="charRarity4">
				<h2>★★★★</h2>
				  <span class="score"></span>
				<div class="btnSelectAll button">Select All</div>				  
				<ul class="rarity4 charList unitList"></ul>
              </section>
              <section id="charRarity3">
				<h2>★★★</h2>
				  <span class="score"></span>
				<div class="btnSelectAll button">Select All</div>				  
				<ul class="rarity3 charList unitList"></ul>
              </section>
              <section id="charRarity2">
				<h2>★★</h2>
				  <span class="score"></span>
				<div class="btnSelectAll button">Select All</div>				  
				<ul class="rarity2 charList unitList"></ul>
              </section>
              <section id=charRarity1>
				<h2>★</h2>
				  <span class="score"></span>
				<div class="btnSelectAll button">Select All</div>				  
				<ul class="rarity1 charList unitList"></ul>
  			  </section>
			  <div id="charGrandTotal">Overall<br><span class="percentage"></span><br><span class="score"></span></div>
          </article>
		  <article id="equips">
              <section id="equipRarity5">
				<h2>★★★★★</h2>
				<span class="score"></span>
				<div class="btnSelectAll button">Select All</div>
				<ul class="rarity5 equipList unitList"></ul>
              </section>	
              <section id="equipRarity4">
				<h2>★★★★</h2>
				<span class="score"></span>
				<div class="btnSelectAll button">Select All</div>
				<ul class="rarity5 equipList unitList"></ul>
              </section>	
              <section id="equipRarity3">
				<h2>★★★</h2>
				<span class="score"></span>
				<div class="btnSelectAll button">Select All</div>
				<ul class="rarity5 equipList unitList"></ul>
              </section>	
              <section id="equipRarity2">
				<h2>★★</h2>
				<span class="score"></span>
				<div class="btnSelectAll button">Select All</div>
				<ul class="rarity5 equipList unitList"></ul>
              </section>	
              <section id="equipRarity1">
				<h2>★</h2>
				<span class="score"></span>
				<div class="btnSelectAll button">Select All</div>
				<ul class="rarity5 equipList unitList"></ul>
              </section>	
			   <div id="equipGrandTotal">Overall<br><span class="percentage"></span><br><span class="score"></span></div>
		  </article>		  
          <div id="info">
              <div class="btnList">
				  <div id="btnAltArt" class="button">
                      Awaken
                  </div>
                  <div id="btnCharInfo" class="button">
                      Unit Info
                  </div>
                  <div id="btnPlanner" class="button">
                      Party Planner
                  </div>	
              </div>			
              <div class="infoWrapper">
              </div>
              <div id="planner" class="plannerWrapper">
                  <ul class="charList unitList">
                  </ul>
                  <ul class="charList unitList">
                  </ul>
                  <li id="btnGetCompURL" class="button">Generate Image URL</li>	
              </div>
          </div>

      </div>
	  <div id="charNamePlate">
		  <div class="ENName"><span></span></div>
		  <div class="JPName"><span></span></div>
		  <div class="Obtain"><span></span></div>
	  </div>
      <div id="charInfoTemplate" class="hidden charInfoBlock" >
          <h1 class="ENName"><span></span></h1>
          <h2 class="JPName"><span></span></h2>
          <div class="expandBlock">
              <div class="Art"></div>
              <div class="details">
                  <div class="Attribute">Attribute: <span></span></div>
                  <div class="Rarity">Rarity: <span></span></div>
                  <div class="LeaderBuff">Leader Skill: <span></span></div>
                  <div class="Skill">Active Skill: <span></span></div>		
                  <div class="Ability1">Ability 1: <span></span></div>		
                  <div class="Ability2">Ability 2: <span></span></div>		
                  <div class="Ability3">Ability 3: <span></span></div>
                  <div class="footer">
                      <div class="Role"><span></span></div>
                      <div class="Gender"><span></span></div>
                      <div class="Race"><span></span></div>	
                  </div>
              </div>				
          </div>

      </div>
      <div id="equipInfoTemplate" class="hidden equipInfoBlock" >
          <h1 class="ENName"><span></span></h1>
          <h2 class="JPName"><span></span></h2>
          <div class="expandBlock">
              <div class="Art"></div>
              <div class="details">
                  <div class="Attribute">Attribute: <span></span></div>
                  <div class="Rarity">Rarity: <span></span></div>
                  <div class="WeaponSkill">Weapon Skill: <span></span></div>		
                  <div class="Obtain">Obtain: <span></span></div>		
              </div>				
          </div>

      </div>	  
	  <script>
		  var listid;
	  		<% if (data.listid) { %> listid = <%= data.listid %>; <% } %>
	  </script>
      <script src="/socket.io/socket.io.js"></script>
      <script src="/js/jquery-3.4.1.min.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/js-cookie@rc/dist/js.cookie.min.js"></script>	
      <script src="/js/eliya.tracker.js"></script>
      <script>

      </script>

      </body>

  </html>